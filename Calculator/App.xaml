<Application x:Class="Calculator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Calculator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
    <SolidColorBrush x:Key="numbersBackground" Color="#444444"/>
    <SolidColorBrush x:Key="numbersForeground" Color="White"/>
    <SolidColorBrush x:Key="operatorsBackground" Color="Orange"/>
    <SolidColorBrush x:Key="operatorsForeground" Color="White"/>
  </Application.Resources>
</Application>
